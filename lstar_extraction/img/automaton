digraph {
	1 [label=start color=black shape=hexagon]
	2 [label=1 color=green]
	3 [label=2 color=black]
	4 [label=3 color=green]
	5 [label=4 color=black]
	6 [label=5 color=green]
	1 -> 2 [label=0]
	1 -> 4 [label=1]
	6 -> 3 [label=0]
	6 -> 6 [label=1]
	3 -> 6 [label=0]
	3 -> 5 [label=1]
	4 -> 3 [label=0]
	4 -> 2 [label=1]
	5 -> 5 [label="0,1"]
	2 -> 2 [label=0]
	2 -> 4 [label=1]
}
